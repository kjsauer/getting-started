### Shell Course #5
$ grep -i "^the" night-b4-fishmas.txt
$ grep -i '^the' night-b4-fishmas.txt
$ cat night-b4-fishmas.txt | grep -i "^the"
$ grep -i ^the night-b4-fishmas.txt
$ grep The *
$ grep "^The" night-b4-fishmas.txt
$ grep -i "^the" night-b4-fishmas.txt | head -n 5
$ grep -i "^the" *
$ grep The night-b4-fishmas.txt
$ cat night-b4-fishmas.txt | grep "^The"
$ grep ^The night-b4-fishmas.txt
$ grep "The" night-b4-fishmas.txt
$ grep '^The' night-b4-fishmas.txt
$ cat night-b4-fishmas.txt | grep "The"
$ grep -i '^the' night-b4-fishmas.txt | head -n 5
$ grep -i "^the" night-b4-fishmas.txt | head -5
$ cat night-b4-fishmas.txt | grep -i '^the'
$ grep -i "^The" night-b4-fishmas.txt
$ grep -i '^the' *
$ cat night-b4-fishmas.txt | grep '^The'
$ cat night-b4-fishmas.txt | grep -i "^the" | head -n 5
$ cat night-b4-fishmas.txt | grep The
$ cat night-b4-fishmas.txt | grep ^The
$ grep -E "^The" night-b4-fishmas.txt
$ cat night-b4-fishmas.txt | grep -i ^the
$ grep -i '^the' night-b4-fishmas.txt | head -5
$ grep -i ^the *
$ grep -iE '^the' night-b4-fishmas.txt
$ grep -E '^The' night-b4-fishmas.txt
$ grep -e "^The" night-b4-fishmas.txt
$ egrep -i '^the' night-b4-fishmas.txt
$ cat night-b4-fishmas.txt | grep -E "^The"
$ egrep -i "^the" night-b4-fishmas.txt
$ grep -i '^the' night-b4-fishmas.txt | head -n5
$ grep -Ei '^the' night-b4-fishmas.txt
$ grep -i "^the" night-b4-fishmas.txt | head -n5
$ grep -iE "^the" night-b4-fishmas.txt
$ grep "The" *
$ grep -Ei "^the" night-b4-fishmas.txt
$ grep -i ^the night-b4-fishmas.txt | head -5
$ cat night-b4-fishmas.txt | grep -i '^the' | head -n 5
$ cat night-b4-fishmas.txt | grep -e "^The"
$ grep -e '^The' night-b4-fishmas.txt
$ grep -i ^the night-b4-fishmas.txt | head -n 5
$ grep 'The' night-b4-fishmas.txt
$ grep -i "^the"  night-b4-fishmas.txt
$ grep -ie "^the" night-b4-fishmas.txt
$ grep '^[Tt]he' night-b4-fishmas.txt
$ cat night-b4-fishmas.txt | grep 'The'
$ grep "^The" *

### Shell Course #6
$ grep '!$' night-b4-fishmas.txt
$ grep "!$" night-b4-fishmas.txt
$ grep -i "!$" night-b4-fishmas.txt
$ grep \!$ night-b4-fishmas.txt
$ grep -i '!$' night-b4-fishmas.txt
$ grep "!" night-b4-fishmas.txt
$ cat night-b4-fishmas.txt | grep -i "!$"
$ grep ! *
$ grep -i "!" night-b4-fishmas.txt
$ cat night-b4-fishmas.txt | grep "!$"
$ grep !$ night-b4-fishmas.txt
$ cat night-b4-fishmas.txt | grep "!"
$ grep "\!$" night-b4-fishmas.txt
$ grep -i "\!$" night-b4-fishmas.txt
$ grep ! night-b4-fishmas.txt
$ grep '!$' *
$ grep "!$" *
$ cat night-b4-fishmas.txt | grep '!$'
$ grep -E '!$' night-b4-fishmas.txt
$ grep -E "!$" night-b4-fishmas.txt
$ grep '\!$' night-b4-fishmas.txt
$ grep '!' night-b4-fishmas.txt
$ cat night-b4-fishmas.txt | grep -i '!$'
$ grep -i "!$" night-b4-fishmas.txt | head -n 6
$ grep -e "!$" night-b4-fishmas.txt
$ cat night-b4-fishmas.txt | grep !$
$ grep -i !$ night-b4-fishmas.txt
$ cat night-b4-fishmas.txt | grep !
$ grep '!$' night-b4-fishmas.txt | head -n 6
$ egrep '!$' night-b4-fishmas.txt
$ grep -i \!$ night-b4-fishmas.txt
$ grep !$ *
$ grep -i "!$" *
$ grep "!$" night-b4-fishmas.txt | head -n 6
$ grep -e '!$' night-b4-fishmas.txt
$ cat night-b4-fishmas.txt | grep -E "!$"
$ grep -i '\!$' night-b4-fishmas.txt
$ cat night-b4-fishmas.txt | grep -i "!"
$ cat night-b4-fishmas.txt | grep -i "\!$"
$ grep "!" *
$ grep '!$' night-b4-fishmas.txt | head -6
$ grep -i '!$' night-b4-fishmas.txt | head -n 6
$ cat night-b4-fishmas.txt | grep '!'
$ grep -i '!$' *
$ egrep "!$" night-b4-fishmas.txt
$ grep -E "\!$" night-b4-fishmas.txt
$ grep  "!$" night-b4-fishmas.txt
$ grep -E '\!$' night-b4-fishmas.txt
$ grep -iE '!$' night-b4-fishmas.txt
$ cat night-b4-fishmas.txt | grep -i !$

