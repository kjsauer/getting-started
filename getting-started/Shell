$ ls -1
$ ls -N night-b4-fishmas.txt
$ echo "$(ls)"
$ ls n*
$ ls -t
$ ls # remember kids, always clean up your inputs <script>console.log('this might be dangerous');</script>
$ ls -u
$ ls -h
$ echo ***
$ ls -N
$ ls -a *
$ echo night-b4-fishmas.txt
$ ls -L
$ ls *txt
$ echo **
$ ls night-b4-fishmas.txt*
$ ls ./
$ ls *.txt
$ echo `ls`
$ ls && wall
$ sleep 3 && ls
$ echo "night-b4-fishmas.txt"
$ ls | grep -E "(skruffl_was_here|night-b4-fishmas)" | awk '{print $NF}'
$ ls -l | grep -E "(skruffl_was_here|night-b4-fishmas)" | awk '{print $NF}'
$ ls **
$ ls -F
$ ls -p
$ ls **/*
$ ls | sort
$ ls # yolo
$ grep -lr x
$ ls #freegaza
$ echo "$(ls .)"
$ perl -e 'exec "ls";'
$ lsattr | sed 's%^[^ ]* \./%%'
$ dir *
$ ls -A1
$ ls;#lsy
$ ls -Ltr
$ ls | cat
$ cd / | ls

### Shell Course #2
$ grep laugh night-b4-fishmas.txt
$ cat night-b4-fishmas.txt | grep laugh
$ grep "laugh" night-b4-fishmas.txt
$ grep -i laugh night-b4-fishmas.txt
$ cat night-b4-fishmas.txt | grep "laugh"
$ grep laugh *
$ grep 'laugh' night-b4-fishmas.txt
$ grep "laugh" *
$ cat night-b4-fishmas.txt | grep 'laugh'
$ cat night-b4-fishmas.txt | grep -i laugh
$ grep -i "laugh" night-b4-fishmas.txt
$ cat night-b4-fishmas.txt|grep laugh
$ cat * | grep laugh
$ grep -i laugh *
$ grep 'laugh' *
$ grep laugh *.txt
$ cat night-b4-fishmas.txt |grep laugh
$ grep laugh night*
$ grep -i Laugh night-b4-fishmas.txt
$ grep laugh ./night-b4-fishmas.txt
$ cat night-b4-fishmas.txt | grep -i "laugh"
$ grep -i 'laugh' night-b4-fishmas.txt
$ grep laugh  night-b4-fishmas.txt
$ cat night-b4-fishmas.txt| grep laugh
$ grep laugh ./*
$ grep laugh < night-b4-fishmas.txt
$ grep "laugh" ./night-b4-fishmas.txt
$ cat * | grep "laugh"
$ grep -i "Laugh" night-b4-fishmas.txt
$ grep laugh ni*
$ grep laugh n*
$ grep -i "laugh" *
$ grep "laugh" ./*
$ grep -e laugh night-b4-fishmas.txt
$ grep "laugh" *.txt
$ grep "laugh" < night-b4-fishmas.txt
$ grep -r "laugh" *
$ cat night-b4-fishmas.txt|grep "laugh"
$ cat night-b4-fishmas.txt|grep -i laugh
$ cat ./night-b4-fishmas.txt | grep laugh
$ grep -rh laugh
$ grep laugh *txt
$ cat night-b4-fishmas.txt |grep "laugh"
$ cat night-b4-fishmas.txt | grep -i Laugh
$ cat night-b4-fishmas.txt | grep -i "Laugh"
$ grep -r laugh *
$ grep  laugh night-b4-fishmas.txt
$ grep "laugh"  night-b4-fishmas.txt
$ grep -i laugh  night-b4-fishmas.txt
$ cat night-b4-fishmas.txt | grep -i 'laugh'

