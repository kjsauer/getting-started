$ head -n3 ./night-b4-fishmas.txt
$ cat night-b4-fishmas.txt|head -n3
$ sed -n '1,3p' night-b4-fishmas.txt
$ cat "night-b4-fishmas.txt" | head -n 3
$ head -n  3 night-b4-fishmas.txt

