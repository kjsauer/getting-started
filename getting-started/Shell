### Shell Course #10
$ sort lords.txt
$ cat lords.txt | sort
$ cat lords.txt|sort
$ sort *
$ sort < lords.txt
$ cat lords.txt |sort
$ cat lords.txt | sort -u
$ cat lords.txt| sort
$ cat lords.txt | sort -n
$ sort <lords.txt
$ cat ./lords.txt | sort
$ sort -u lords.txt
$ sort ./lords.txt
$ cat lords.txt | sort | head -n 10
$ sort l*
$ cat lords.txt | sort -d
$ sort lords.txt | head -n 10
$ cat * | sort
$ sort  lords.txt
$ sort -h ./lords.txt
$ cat lords.txt | sort -h
$ sort -d lords.txt
$ cat lords.txt | sort | head -10
$ sort lords*
$ sort -h lords.txt
$ cat lords* | sort
$ head -n 10 lords.txt | sort
$ find . -type f -exec sort {} \;
$ <lords.txt sort
$ sort -n lords.txt
$ sort -k2 lords.txt
$ sort lords.txt | cat
$ cat lords.txt | sort -k2
$ sort lords.txt | head -10
$ cat lords.txt | sort | uniq
$ echo "$(<lords.txt)" | sort
$ cat lords.txt | uniq | sort
$ cat $(find -type f | sort) | sort
$ sort <*
$ sort lo*
$ head lords.txt | sort
$ cat lords.txt|sort -k2
$ find . -type f | xargs sort
$ cat lords.txt | sort | head
$ cat *|sort
$ sort -u < lords.txt
$ sort -k2,2 lords.txt
$ cat lords.txt|sort -h
$ cat  lords.txt | sort
$ cat lords.txt  | sort

### Shell Course #11
$ find . -name "piper" -type f -exec grep piping {} \;
$ grep -hr p p*
$ find . -name piper -type f -exec grep piping {} \;
$ cat place-for-pipers/*/piper | grep piping
$ grep -hR piping place*
$ cat place-for-pipers/*/* | grep piping
$ find place-for-pipers -type f -exec grep piping {} \;
$ find . -name piper -exec grep piping {} \;
$ find . -type f -name "piper" -exec grep piping {} \;
$ find . -name 'piper' -type f -exec grep piping {} \;
$ find . -name "piper" -exec grep piping {} \;
$ find . -type f -name piper -exec grep piping {} \;
$ find . -name "piper" -type f -exec grep "piping" {} \;
$ grep -hr piping p*
$ find place-for-pipers -type f -exec cat {} \; | grep piping
$ grep -hr piping place-for-pipers
$ find place-for-pipers -type f | xargs cat | grep piping
$ find -name "piper" -type f -exec grep piping {} \;
$ grep -h piping place-for-pipers/*/piper
$ cat ***/***/* | grep pip
$ grep -hr piping place*
$ find -name "piper" -exec grep piping {} \;
$ cat place-for-pipers/*/piper|grep piping
$ cat place-for-pipers/*/piper | grep "piping"
$ find . -type f -name 'piper' -exec grep piping {} \;
$ grep -hr pi p*
$ cat place-for-pipers/*/* | grep pip
$ cat place-for-pipers/*/piper | grep pip
$ find -name piper -exec grep piping {} \;
$ cat place-for-pipers/*/* | grep "piping"
$ cat place-for-pipers/**/piper | grep piping
$ find . -name 'piper' -exec grep piping {} \;
$ grep -hr pip p*
$ find . -name "piper" -exec grep "piping" {} \;
$ cat */*/* | grep pip
$ find ./place-for-pipers -type f -exec grep piping {} \;
$ grep -hr "piping" **/piper
$ find . -name piper | xargs cat | grep piping
$ find -type f -name piper -exec grep piping {} \;
$ find . -type f -name 'piper' -exec grep 'piping' {} \;
$ grep -hr "piping" place-for-pipers
$ find . -type f -name "piper" -exec grep "piping" {} \;
$ grep -rh "piping " place-for-pipers
$ find -name piper -type f -exec grep piping {} \;
$ ls ./place-for-pipers | xargs -I x grep 'piping' './place-for-pipers/x/piper'
$ rgrep -h  pip place*
$ grep -h piping place-for-pipers/*/*
$ find -name "piper" -exec cat {} \; | grep pip
$ find -name "piper" -type f -exec grep "piping" {} \;
$ find -type f -name "piper" -exec cat {} \; | grep piping

### Shell Course #12
$ cat twelve-days-of-shell.txt
$ cat *
$ echo "$(<twelve-days-of-shell.txt)"
$ cat t*
$ cat twelve*
$ grep . twelve-days-of-shell.txt
$ cat tw*
$ cat twe*
$ echo "$(< twelve-days-of-shell.txt)"
$ cat  twelve-days-of-shell.txt
$ cat twel*
$ find . -name twelve-days-of-shell.txt -type f -exec cat {} \;
$ echo "$(<twelve*)"
$ uniq twelve-days-of-shell.txt
$ cat twelve*.txt
$ find * -name "twelve-days-of-shell.txt" -type f -exec cat twelve-days-of-shell.txt \;
$ cat *.txt
$ cat "twelve-days-of-shell.txt"
$ cat **/*twelve-days-of-shell.txt
$ printf "%s" "$(<twelve-days-of-shell.txt)"
$ cat ./*
$ cat ./twelve-days-of-shell.txt
$ cat 'twelve-days-of-shell.txt'
$ find /  -name twelve* | xargs cat
$ grep " " twelve-days-of-shell.txt
$ cat twelve-days-of-shell.txt | head -n 1000
$ cat *t
$ <* cat
$ cat *txt
$ cat $(ls)
$ echo "$(<t*)"
$ cat ./twelve*
$ cat twelve-d*
$ echo "$(< twelve-*)"
$ cat twelve-days-of-shell.txt;
$ find . -type f -exec cat {} \;
$ head -20 twelve-days-of-shell.txt
$ head -n 16 twelve-days-of-shell.txt
$ tail -n 100 twelve-days-of-shell.txt
$ find -name "twelve-days-of-shell.txt" -exec cat {} \;
$ cat t*.txt
$ cat tw*txt
$ cat tw*.txt
$ cat twel*.txt
$ grep . twelve*
$ cat twelve-da*.txt
$ tail -n 10000 twelve*
$ for say in 0#; do cat *; done
$ cat * >/tmp/hahaha && cat /tmp/*
$ tail -n 20 twelve-days-of-shell.txt