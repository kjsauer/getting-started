$ ls | grep -i '^Santa'
$ ls | grep -i santa
$ ls S*
$ ls Santa* | head -7
$ find -name 'Santa*'
$ ls [sS]anta*
$ ls | grep -E '^Santa'
$ ls | grep -E "^Santa"
$ ls | grep "^Santa" | head -n 7
$ ls | grep -i '^santa'
$ ls | grep -e "^Santa"
$ find -type f -name "Santa*"
$ ls | grep -i "santa"
$ ls -1 | grep Santa
$ ls | grep -i ^santa
$ find . -iname "Santa*"
$ ls San*
$ ls Sant*
$ ls -1 Santa*
$ ls |grep Santa
$ ls | grep -i "^Santa" | head -n 7
$ ls Santa* | head -n7
$ find . -iname "santa*"
$ ls  Santa*
$ ls | grep -i ^Santa
$ ls | grep -i "Santa"
$ find ./ -name "Santa*"
$ find -type f -name 'Santa*'
$ ls ./Santa*
$ ls -d Santa*
$ ls|grep Santa
$ ls | grep "Santa*"
$ ls | grep '^Santa' | head -n 7

### Shell Course #8
$ mv Elves/* Workshop/
$ mv Elves/* Workshop
$ mv Elves/* Workshop/.
$ mv ./Elves/* ./Workshop
$ mv ./Elves/* ./Workshop/
$ mv Elves/* ./Workshop
$ mv Elves/* ./Workshop/
$ find Elves -type f -exec mv {} Workshop \;
$ find Elves -type f -exec mv {} Workshop/ \;
$ find Elves/ -type f -exec mv {} Workshop/ \;
$ mv  Elves/* Workshop/
$ find . -name Elves | mv Elves/* Workshop/
$ mv  Elves/* Workshop
$ mv Elves/*  Workshop/
$ mv ./Elves/* Workshop/
$ mv ./Elves/* Workshop
$ mv -t Workshop Elves/*
$ find Elves/ -type f -exec bash -c 'mv "$1" Workshop/' - {} \;
$ ls Elves/ | mv Elves/* Workshop/
$ find . -type f -exec mv {} Workshop/ \;
$ find ./Elves -type f -exec mv {} ./Workshop \;
$ mv Elves/* Workshop*
$ mv Elves/* -t Workshop/
$ mv Elves/*  Workshop
$ mv Elves/* -t Workshop
$ mv Elves/*   Workshop/
$ mv "Elves"/* Workshop/
$ mv E*/* W*/
$ mv Elves/* Work*
$ find ./Elves -type f -exec mv {} Workshop \;
$ ls Elves | xargs -I x mv './Elves/x' Workshop
$ find ./Elves -type f -exec mv {} Workshop/ \;
$ mv [Ee]lves/* Workshop/
$ cd Elves; mv * ../Workshop
$ ls Elves/| mv Elves/* Workshop
$ mv Elves/* Workshop && ls Workshop
$ mv -v Elves/* Workshop/
$ mv -i Elves/* Workshop/
$ mv ./Elves/* ./Workshop/.
$ mv elves Elves/* Workshop/
$ mv ./Elves/* -t ./Workshop
$ cd Elves && mv * ../Workshop
$ find ./Elves -type f | mv Elves/* Workshop/
$ find Elves/ -type f -exec mv {} Workshop \;
$ ls Elves | xargs -I x mv "./Elves/x" Workshop
$ find ./ -type f -name '*' -exec mv '{}' ./Workshop \;
$ mv -f Elves/* Workshop
$ mv ls Elves/* Workshop/
$ mv -t Workshop/ Elves/*
$ cd Elves;mv * ../Workshop

